<xs:schema xmlns="RepositoryContent" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="RepositoryContent" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:redefine schemaLocation="bin/resources/TConcepts.xsd">
		<!-- ========== APPLICATION ============= -->
		<xs:complexType name="Application">
			<xs:complexContent>
				<xs:extension base="Application">
					<xs:sequence>
						<xs:element name="component" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="Component">
										<xs:sequence>
											<xs:element name="implementation" minOccurs="1" maxOccurs="unbounded">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="Implementation">
															<xs:attribute name="ID" type="xs:string" use="required"/>
														</xs:extension>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="ID" type="xs:string" use="required"/>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="ID" type="xs:string" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<!-- ============= APPLICATIONSET =========== -->
		<xs:complexType name="ApplicationSet">
			<xs:complexContent>
				<xs:extension base="ApplicationSet">
					<xs:sequence>
						<xs:element name="application" type="Application" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="ID" type="xs:string" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:redefine>
	<!--elements that can be validated-->
	<xs:element name="applicationSet" type="ApplicationSet"/>
</xs:schema>
