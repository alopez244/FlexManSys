<xs:schema xmlns="ManufacturingPlan" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" targetNamespace="ManufacturingPlan" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="PlannedItem.xsd"/>
	<xs:redefine schemaLocation="../AppProperties.xsd">
		<!-- ============= BATCH =========== -->
		<xs:complexType name="Batch">
			<xs:complexContent>
				<xs:extension base="Batch">
					<xs:sequence>
						<xs:element ref="PlannedItem" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<!-- ============= ORDER =========== -->
		<xs:complexType name="Order">
			<xs:complexContent>
				<xs:extension base="Order">
					<xs:sequence>
						<xs:element name="batch" type="Batch" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<!-- ============= MANUFACTURING=========== -->
		<xs:complexType name="MPlan">
			<xs:complexContent>
				<xs:extension base="MPlan">
					<xs:sequence>
						<xs:element name="order" type="Order" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:redefine>
	<!--system elements that can be validated-->
	<xs:element name="mPlan" type="MPlan"/>
</xs:schema>
